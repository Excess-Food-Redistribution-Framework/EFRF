{
  "name": "frontend-web",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext js,cjs,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,cjs,ts,tsx --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "prepare": "cd .. && husky install frontend-web/.husky",
    "import-articles": "ts-node --esm ./src/scripts/importArticles.ts src/data/articles.json"
  },
  "dependencies": {
    "@react-google-maps/api": "^2.19.2",
    "axios": "^1.5.1",
    "bootstrap": "^5.3.2",
    "country-state-city": "^3.2.1",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.17.0",
    "react-select": "^5.8.0",
    "react-spring": "^9.7.3",
    "react-toastify": "^9.1.3"
  },
  "devDependencies": {
    "@types/qs": "^6.9.11",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-modal": "^3.16.3",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.5"
  }
}
